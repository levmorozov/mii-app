{
  "name": "mii-app",
  "description": "mii simple application skeleton",
  "type": "project",
  "minimum-stability": "dev",
  "license": "MIT",
  "require": {
    "php": ">=5.6.0",
    "levmorozov/mii": "dev-master",
    "bower-asset/jquery": "1.*.*@stable"
  },
  "autoload": {
    "psr-4": {
      "app\\" : "app/"
    }
  },
  "extra": {
    "installer-paths": {
      "mii\\": "vendor/mii"
    },
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
